</addon><addon id="plugin.video.nba" name="NBA League Pass II (kokan)" version="1.0.2" provider-name="ivankokan, maxgalbu, ptsampoukas, robla">
  <requires>
    <import addon="script.common.plugin.cache" version="0.9.1" />
    <import addon="script.module.dateutil" version="2.8.1" />
    <import addon="script.module.pytz" version="2014.2" />
